  "explorer.confirmDelete": false,
  "debug.inlineValues": true,
  "javascript.suggestionActions.enabled": false,
  "debug.openDebug": "openOnSessionStart",
  "editor.detectIndentation": false,
  "editor.snippetSuggestions": "top",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.showUpdateMessage": false,

  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.schema": "json",
    "*.json": "jsonc",
    "*.flatschema": "jsonc",
    "*.flat": "jsonc",
    "*.model": "jsonc",
    "/Users/cameron/.config/*": "ini"
  },
  "html.suggest.html5": false,
  "php.suggest.basic": false,
  "javascript.suggest.names": false,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.acceptSuggestionOnEnter": "off",
  // "editor.largeFileOptimizations": false,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    // "source.organizeImports": true,
    // "source.fixAll": true
  },

  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "task.problemMatchers.neverPrompt": {
    "shell": true
  },
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "HookyQR.beautify",
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "bookmarks.label.suggestion": "suggestWhenSelectedOrLineWhenNoSelected",
  "editor.cursorSurroundingLines": 10,
  "editor.find.autoFindInSelection": "multiline",
  "editor.formatOnSaveMode": "file",
  "diffEditor.renderIndicators": false,
  "diffEditor.wordWrap": "off",
  "editor.minimap.renderCharacters": false,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.wordBasedSuggestionsMode": "allDocuments",
  "files.autoGuessEncoding": true,
  "files.hotExit": "onExitAndWindowClose",
  "workbench.editor.decorations.colors": true,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "always",
  "breadcrumbs.enabled": false,
  "workbench.editor.focusRecentEditorAfterClose": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.splitSizing": "split",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": true,
  "window.autoDetectHighContrast": false,
  "window.nativeTabs": true,
  "window.titleBarStyle": "native",
  "explorer.decorations.badges": false,
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.sortOrder": "alphabetical",
  "eslint.format.enable": true,
  "material-icon-theme.activeIconPack": "nest",
  "merge-conflict.codeLens.enabled": false,
  "materialTheme.accent": "Pink",
  "prettier.arrowParens": "avoid",
  "tsco.groupPropertiesWithDecorators": true,
  "outline.showEnums": false,
  "outline.showInterfaces": false,
  "outline.showFunctions": false,
  "outline.showProperties": false,
  "editor.suggest.showSnippets": false,
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "editor.suggest.showWords": false,
  "git.autoRepositoryDetection": false,
  "git.branchWhitespaceChar": "_",
  "git.fetchOnPull": true,
  "git.untrackedChanges": "separate",
  "git.useCommitInputAsStashMessage": true,
  "gitlens.advanced.fileHistoryFollowsRenames": true,
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.defaultGravatarsStyle": "monsterid",
  "gitlens.fileAnnotations.command": "changes",
  "gitlens.heatmap.toggleMode": "window",
  // "gitlens.hovers.annotations.over": "line",
  "git.autofetch": true,
  "terminal.integrated.rendererType": "dom",
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    }
  ],
  "editor.minimap.enabled": false,
  "terminal.integrated.scrollback": 1000000,
  "git.confirmSync": false,
  "explorer.confirmDragAndDrop": false,

  "git-graph.dialog.resetCurrentBranchToCommit.mode": "Hard",
  "git-graph.dialog.resetUncommittedChanges.mode": "Hard",
  "workbench.colorTheme": "GitHub Dark Default",
  "debug.javascript.suggestPrettyPrinting": false,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
    "branch-create:command"
  ],
  "redhat.telemetry.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "editor.multiCursorModifier": "ctrlCmd",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.renderIndentGuides": false,
  "editor.highlightActiveIndentGuide": false,
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  "editor.wordWrap": "off",
  "[markdown]": {
    "editor.wordWrap": "off",
    "editor.quickSuggestions": false
  },
}
