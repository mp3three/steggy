{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "unicorn/no-null": "off"
      }
    },
    {
      "files": ["src/mock/**/*ts"],
      "rules": {
        "radar/no-duplicate-string": "off"
      }
    }
  ]
}
