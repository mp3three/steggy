{
  "root": "libs/formio-sdk",
  "sourceRoot": "libs/formio-sdk/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/formio-sdk/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/formio-sdk"],
      "options": {
        "jestConfig": "libs/formio-sdk/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/formio-sdk",
        "tsConfig": "libs/formio-sdk/tsconfig.lib.json",
        "packageJson": "libs/formio-sdk/package.json",
        "main": "libs/formio-sdk/src/index.ts",
        "assets": ["libs/formio-sdk/*.md"]
      }
    }
  },
  "tags": []
}
