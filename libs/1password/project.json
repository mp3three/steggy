{
  "root": "libs/1password",
  "sourceRoot": "libs/1password/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/1password",
        "tsConfig": "libs/1password/tsconfig.lib.json",
        "packageJson": "libs/1password/package.json",
        "main": "libs/1password/src/index.ts",
        "assets": ["libs/1password/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/1password/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/1password"],
      "options": {
        "jestConfig": "libs/1password/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
