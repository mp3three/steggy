{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "@automagical/licenses",
  "type": "object",
  "additionalProperties": false,
  "$id": "licenses.json",
  "properties": {
    "ADMIN_TOKEN": {
      "description": "API token to allow access to a license providing API Server",
      "type": "string"
    },
    "TOKEN_HEADER": {
      "description": "Owner token",
      "type": "string",
      "default": "x-admin-token"
    }
  }
}
