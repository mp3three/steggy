{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "home-controller",
  "projects": {
    "controller-logic": "libs/controller-logic",
    "devtools": "apps/devtools",
    "home-assistant": "libs/home-assistant",
    "home-controller": "apps/home-controller",
    "living-docs": "apps/living-docs",
    "persistence": "libs/persistence",
    "server": "libs/server",
    "tty": "libs/tty",
    "utilities": "libs/utilities"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  }
}
