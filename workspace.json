{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/nest" },
  "defaultProject": "home-controller",
  "projects": {
    "home-controller": {
      "root": "apps/home-controller",
      "sourceRoot": "apps/home-controller/src",
      "projectType": "application",
      "targets": {
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx webpack-dev home-controller --inspect --port 33333 | pino-pretty"
          }
        },
        "webpack-dev": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "home-controller:build"
          }
        },
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/home-controller",
            "generatePackageJson": true,
            "main": "apps/home-controller/src/entrypoints/main.ts",
            "tsConfig": "apps/home-controller/tsconfig.app.json"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/home-controller/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/home-controller"
          ],
          "options": {
            "jestConfig": "apps/home-controller/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "authentication": {
      "root": "libs/authentication",
      "sourceRoot": "libs/authentication/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/authentication/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/authentication"
          ],
          "options": {
            "jestConfig": "libs/authentication/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/authentication",
            "tsConfig": "libs/authentication/tsconfig.lib.json",
            "packageJson": "libs/authentication/package.json",
            "main": "libs/authentication/src/index.ts",
            "assets": [
              "libs/authentication/*.md"
            ]
          }
        }
      }
    },
    "contracts": {
      "root": "libs/contracts",
      "sourceRoot": "contracts",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
