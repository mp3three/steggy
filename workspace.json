{
  "version": 2,
  "projects": {
    "type-writer": {
      "root": "libs/type-writer",
      "sourceRoot": "libs/type-writer/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/type-writer/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/type-writer"
          ],
          "options": {
            "jestConfig": "libs/type-writer/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/type-writer",
            "tsConfig": "libs/type-writer/tsconfig.lib.json",
            "packageJson": "libs/type-writer/package.json",
            "main": "libs/type-writer/src/index.ts",
            "assets": [
              "libs/type-writer/*.md"
            ]
          }
        }
      }
    },
    "logger": {
      "root": "libs/logger",
      "sourceRoot": "libs/logger/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/logger/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/logger"
          ],
          "options": {
            "jestConfig": "libs/logger/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "validation": {
      "root": "libs/validation",
      "sourceRoot": "libs/validation/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/validation/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/validation"
          ],
          "options": {
            "jestConfig": "libs/validation/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/validation",
            "tsConfig": "libs/validation/tsconfig.lib.json",
            "packageJson": "libs/validation/package.json",
            "main": "libs/validation/src/index.ts",
            "assets": [
              "libs/validation/*.md"
            ]
          }
        }
      }
    },
    "home-assistant": {
      "root": "libs/home-assistant",
      "sourceRoot": "libs/home-assistant/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/home-assistant/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/home-assistant"
          ],
          "options": {
            "jestConfig": "libs/home-assistant/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/home-assistant",
            "tsConfig": "libs/home-assistant/tsconfig.lib.json",
            "packageJson": "libs/home-assistant/package.json",
            "main": "libs/home-assistant/src/index.ts",
            "assets": [
              "libs/home-assistant/*.md"
            ]
          }
        }
      }
    },
    "home-controller": {
      "root": "apps/home-controller",
      "sourceRoot": "apps/home-controller/src",
      "projectType": "application",
      "prefix": "home-controller",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/home-controller",
            "main": "apps/home-controller/src/main.ts",
            "tsConfig": "apps/home-controller/tsconfig.app.json",
            "assets": [
              "apps/home-controller/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/home-controller/src/environments/environment.ts",
                  "with": "apps/home-controller/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "home-controller:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/home-controller/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/home-controller"
          ],
          "options": {
            "jestConfig": "apps/home-controller/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "home-controller"
}