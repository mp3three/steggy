{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "home-controller",
  "projects": {
    "contracts": {
      "root": "libs/contracts",
      "sourceRoot": "contracts",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/contracts",
            "tsConfig": "libs/contracts/tsconfig.lib.json",
            "packageJson": "libs/contracts/package.json",
            "main": "libs/contracts/src/index.ts"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/contracts/**/*.ts"]
          }
        }
      }
    },
    "testing": {
      "root": "libs/testing",
      "sourceRoot": "libs/testing/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/testing",
            "tsConfig": "libs/testing/tsconfig.lib.json",
            "packageJson": "libs/testing/package.json",
            "main": "libs/testing/src/index.ts",
            "assets": [
              "libs/testing/*.md"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/testing/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/testing"
          ],
          "options": {
            "jestConfig": "libs/testing/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "devtools": {
      "root": "apps/devtools",
      "sourceRoot": "apps/devtools/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/devtools",
            "main": "apps/devtools/src/main.ts",
            "tsConfig": "apps/devtools/tsconfig.app.json",
            "assets": ["apps/devtools/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/devtools/src/environments/environment.ts",
                  "with": "apps/devtools/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "devtools:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/devtools/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/devtools"],
          "options": {
            "jestConfig": "apps/devtools/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "formio-sdk": {
      "root": "libs/formio-sdk",
      "sourceRoot": "libs/formio-sdk/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/formio-sdk/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/formio-sdk"],
          "options": {
            "jestConfig": "libs/formio-sdk/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/formio-sdk",
            "tsConfig": "libs/formio-sdk/tsconfig.lib.json",
            "packageJson": "libs/formio-sdk/package.json",
            "main": "libs/formio-sdk/src/index.ts",
            "assets": ["libs/formio-sdk/*.md"]
          }
        }
      }
    },
    "home-assistant": {
      "root": "libs/home-assistant",
      "sourceRoot": "libs/home-assistant/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/home-assistant/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/home-assistant"],
          "options": {
            "jestConfig": "libs/home-assistant/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/home-assistant",
            "tsConfig": "libs/home-assistant/tsconfig.lib.json",
            "packageJson": "libs/home-assistant/package.json",
            "main": "libs/home-assistant/src/index.ts",
            "assets": ["libs/home-assistant/*.md"]
          }
        }
      }
    },
    "home-controller": {
      "root": "apps/home-controller",
      "sourceRoot": "apps/home-controller/src",
      "projectType": "application",
      "targets": {
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx webpack-dev home-controller --inspect --port 33333 | pino-pretty"
          }
        },
        "webpack-dev": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "home-controller:build"
          }
        },
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/home-controller",
            "generatePackageJson": true,
            "main": "apps/home-controller/src/entrypoints/main.ts",
            "tsConfig": "apps/home-controller/tsconfig.app.json"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/home-controller/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/home-controller"],
          "options": {
            "jestConfig": "apps/home-controller/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "living-docs": {
      "root": "apps/living-docs",
      "sourceRoot": "apps/living-docs/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/living-docs",
            "main": "apps/living-docs/src/main.ts",
            "tsConfig": "apps/living-docs/tsconfig.app.json",
            "assets": ["apps/living-docs/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/living-docs/src/environments/environment.ts",
                  "with": "apps/living-docs/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "living-docs:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/living-docs/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/living-docs"],
          "options": {
            "jestConfig": "apps/living-docs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "persistence": {
      "root": "libs/persistence",
      "sourceRoot": "libs/persistence/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/persistence/**/*.ts"]
          }
        },
        "test-pretty": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              {
                "command": "nx test persistence | pino-pretty"
              }
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/persistence"],
          "options": {
            "jestConfig": "libs/persistence/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/persistence",
            "tsConfig": "libs/persistence/tsconfig.lib.json",
            "packageJson": "libs/persistence/package.json",
            "main": "libs/persistence/src/index.ts",
            "assets": ["libs/persistence/*.md"]
          }
        }
      }
    },
    "server": {
      "root": "libs/server",
      "sourceRoot": "libs/server/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/server/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/server"],
          "options": {
            "jestConfig": "libs/server/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/server",
            "tsConfig": "libs/server/tsconfig.lib.json",
            "packageJson": "libs/server/package.json",
            "main": "libs/server/src/index.ts",
            "assets": ["libs/server/*.md"]
          }
        }
      }
    },
    "terminal": {
      "root": "libs/terminal",
      "sourceRoot": "libs/terminal/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/terminal/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/terminal"],
          "options": {
            "jestConfig": "libs/terminal/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "utilities": {
      "root": "libs/utilities",
      "sourceRoot": "libs/utilities/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utilities",
            "tsConfig": "libs/utilities/tsconfig.lib.json",
            "packageJson": "libs/utilities/package.json",
            "main": "libs/utilities/src/index.ts"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utilities/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utilities"],
          "options": {
            "jestConfig": "libs/utilities/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
