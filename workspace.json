    "home-controller": {
      "root": "apps/home-controller",
      "sourceRoot": "apps/home-controller/src",
      "projectType": "application",
      "prefix": "home-controller",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              {
                "command": "nx run home-controller:build-app:production"
              },
              {
                "command": "echo; echo; echo Copying user-env files to assets; echo; echo;"
              },
              {
                "command": "cp user-env.yaml dist/apps/home-controller/assets/"
              },
              {
                "command": "cp user-env.production.yaml dist/apps/home-controller/assets/"
              }
            ]
          }
        },
        "build-app": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/home-controller",
            "main": "apps/home-controller/src/main.ts",
            "tsConfig": "apps/home-controller/tsconfig.app.json",
            "assets": [
              "apps/home-controller/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "generatePackageJson": true,
              "showCircularDependencies": false,
              "statsJson": true,
              "fileReplacements": [
                {
                  "replace": "apps/home-controller/src/environments/environment.ts",
                  "with": "apps/home-controller/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "home-controller:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/home-controller/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/home-controller"
          ],
          "options": {
            "jestConfig": "apps/home-controller/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
