{
  "root": "apps/automations",
  "sourceRoot": "apps/automations/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/automations",
        "index": "apps/automations/src/index.html",
        "defaultConfiguration": "development",
        "baseHref": "/",
        "main": "apps/automations/src/main.tsx",
        "polyfills": "apps/automations/src/polyfills.ts",
        "tsConfig": "apps/automations/tsconfig.app.json",
        "assets": [
          "apps/automations/src/favicon.ico",
          "apps/automations/src/assets"
        ],
        "styles": ["apps/automations/src/styles.css"],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "defaultConfiguration": "production",
          "fileReplacements": [
            {
              "replace": "apps/automations/src/environments/environment.ts",
              "with": "apps/automations/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "automations:build:development",
        "hmr": true,
        "host": "0.0.0.0"
      },
      "configurations": {
        "production": {
          "buildTarget": "automations:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/automations/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/automations"],
      "options": {
        "jestConfig": "apps/automations/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
