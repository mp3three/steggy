{
  "root": "apps/dashboard",
  "sourceRoot": "apps/dashboard/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/dashboard",
        "main": "apps/dashboard/src/entrypoints/main.ts",
        "inspect": true,
        "tsConfig": "apps/dashboard/tsconfig.app.json",
        "fileReplacements": [
         {
           "replace": "apps/dashboard/src/environments/environment.ts",
           "with": "apps/dashboard/src/environments/develop.environment.ts"
         }
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": []
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "dashboard:build",
        "inspect": "inspect",
        "port": 33335
      }
    },
    "brk": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npx nx serve dashboard --inspect inspect-brk"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/dashboard/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/dashboard"],
      "options": {
        "jestConfig": "apps/dashboard/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
