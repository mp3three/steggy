{
  "root": "apps/home-controller",
  "sourceRoot": "apps/home-controller/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "nx webpack-dev home-controller --inspect --port 33334 | pino-pretty"
      }
    },
    "inspect": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "nx webpack-dev home-controller --inspect inspect-brk --port 33334 | pino-pretty"
      }
    },
    "webpack-dev": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "home-controller:build"
      }
    },
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/home-controller",
        "generatePackageJson": true,
        "main": "apps/home-controller/src/entrypoints/main.ts",
        "tsConfig": "apps/home-controller/tsconfig.app.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/home-controller/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/home-controller"],
      "options": {
        "jestConfig": "apps/home-controller/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
