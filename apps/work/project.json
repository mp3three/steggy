{
  "root": "apps/work",
  "sourceRoot": "apps/work/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "work:build",
        "inspect": "inspect",
        "port": 33335
      }
    },
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "assets": [
          {
            "input": "libs/utilities",
            "glob": "*.json",
            "output": "./assets/utilities"
          },
          {
            "input": "libs/boilerplate",
            "glob": "*.json",
            "output": "./assets/boilerplate"
          },
          {
            "input": "libs/formio",
            "glob": "*.json",
            "output": "./assets/formio"
          },
          {
            "input": "libs/tty",
            "glob": "*.json",
            "output": "./assets/tty"
          },
          {
            "input": "libs/server",
            "glob": "*.json",
            "output": "./assets/server"
          },
          {
            "input": "apps/work",
            "glob": "*.json",
            "output": "./assets/work"
          },
          {
            "input": "./",
            "glob": "workspace.json",
            "output": "./assets"
          }
        ],
        "outputPath": "dist/apps/work",
        "generatePackageJson": true,
        "main": "apps/work/src/develop.main.ts",
        "tsConfig": "apps/work/tsconfig.app.json"
      },
      "configurations": {
        "offline-license": {
          "main": "apps/work/src/offline-license.service.ts",
          "optimization": true,
          "extractLicenses": false,
          "inspect": false
        },
        "pdf-downloader": {
          "main": "apps/work/src/pdf-downloader.service.ts",
          "optimization": true,
          "extractLicenses": false,
          "inspect": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/work/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
