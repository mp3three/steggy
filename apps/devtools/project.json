{
  "root": "apps/devtools",
  "sourceRoot": "apps/devtools/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "devtools:build",
        "inspect": "inspect",
        "port": 33336
      }
    },
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/devtools",
        "generatePackageJson": true,
        "main": "apps/devtools/src/entrypoints/main.ts",
        "tsConfig": "apps/devtools/tsconfig.app.json"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": []
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/devtools/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/devtools"],
      "options": {
        "jestConfig": "apps/devtools/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
