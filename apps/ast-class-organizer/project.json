{
  "root": "apps/ast-class-organizer",
  "sourceRoot": "apps/ast-class-organizer/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ast-class-organizer",
        "main": "apps/ast-class-organizer/src/entrypoints/main.ts",
        "tsConfig": "apps/ast-class-organizer/tsconfig.app.json",
        "assets": ["apps/ast-class-organizer/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "generatePackageJson": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/ast-class-organizer/src/environments/environment.ts",
              "with": "apps/ast-class-organizer/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "webpack-dev": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "ast-class-organizer:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/ast-class-organizer/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/ast-class-organizer"],
      "options": {
        "jestConfig": "apps/ast-class-organizer/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build-vsix": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "rm dist/apps/ast-class-organizer/*vsix;echo"
          },
          {
            "command": "cd apps/ast-class-organizer;pwd"
          },
          {
            "command": "npx vsce package"
          },
          {
            "command": "mv *vsix ../../dist/apps/ast-class-organizer"
          }
        ]
      }
    },
    "vscode": {
      "executor": "@nrwl/workspace:run-commands",
      "configurations": {
        "postinstall": {
          "command": "node ./node_modules/vscode/bin/install"
        },
        "test": {
          "commands": [
            {
              "command": "npx nx build ast-class-organizer"
            },
            {
              "command": "node ./node_modules/vscode/bin/test"
            }
          ]
        }
      }
    }
  },
  "tags": []
}
