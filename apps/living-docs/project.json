{
  "root": "apps/living-docs",
  "sourceRoot": "apps/living-docs/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/living-docs",
        "main": "apps/living-docs/src/main.ts",
        "tsConfig": "apps/living-docs/tsconfig.app.json",
        "assets": ["apps/living-docs/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/living-docs/src/environments/environment.ts",
              "with": "apps/living-docs/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "living-docs:build"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/living-docs"],
      "options": {
        "jestConfig": "apps/living-docs/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
